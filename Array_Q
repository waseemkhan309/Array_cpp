//Q: Add element of two arrays.
#include<iostream>
using namespace std;

int main(){
    int A[2],B[2],result=0;
    
    // input
    for(int i=0;i<2;i++){
        cin>>A[i];
    }
    for(int i=0;i<2;i++){
        cin>>B[i];
    }
    
    // output
    for(int i=0;i<2;i++){
        cout<<A[i];
    }
    cout<<endl;
    for(int i=0;i<2;i++){
        cout<<B[i];
    }
    // Addition
    for(int i=0;i<2;i++){
        result += A[i];
    }
    for(int i=0;i<2;i++){
        result += B[i];
    }
    cout<<endl;
    cout<<result<<endl;
    return 0;
}
